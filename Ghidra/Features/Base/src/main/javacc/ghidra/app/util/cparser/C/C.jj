            }
        )
        { lastDataType = dec.getDataType(); }
        (
            (
                "[" [ obj = ConditionalExpression() ] "]" 
                {
                    // make a new array given value on constant expression
                    Integer size = getConstantValue(obj, 0);
                    if (size == null) {
                        System.out.println("BAD ARRAY SIZE! " + obj);
                        size = 0;
                    }
                    list.add(0,size);
                }
