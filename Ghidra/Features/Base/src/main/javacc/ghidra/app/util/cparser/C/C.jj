Declaration DeclarationSpecifiers(Declaration specDT) : {
    Declaration typeDT= null;
}
{
    (    
        specDT = StorageClassSpecifier(specDT) 
        [
//            LOOKAHEAD(DeclarationSpecifiers(specDT))
            specDT= DeclarationSpecifiers(specDT) 
