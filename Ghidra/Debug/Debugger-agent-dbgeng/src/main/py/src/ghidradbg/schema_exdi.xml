	</schema>
	<schema name="ProcessContainer" canonical="yes" elementResync="NEVER" attributeResync="NEVER">
		<element schema="Process" />
		<attribute name="_order" schema="INT" hidden="yes" />
		<attribute schema="ANY" />
	</schema>
