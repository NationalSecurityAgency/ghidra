:e_ldmvgprw d8PlusRaOrZeroAddress is $(ISVLE) & OP=6 & d8PlusRaOrZeroAddress & XOP_8_VLE=0x10 & BITS_21_25=0
{
	tea = d8PlusRaOrZeroAddress;
	loadReg(r0);
	loadReg(r3);
	loadReg(r4);
	loadReg(r5);
	loadReg(r6);
	loadReg(r7);
	loadReg(r8);
