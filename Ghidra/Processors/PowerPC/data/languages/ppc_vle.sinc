:e_rlwimi A,S,SHL,MBL,ME			is $(ISVLE) & OP=29 & BIT_0=0 & MBL & ME & A & SHL & S {
	tmpS:4 = S:4;
	tmpA:4 = (tmpS << SHL) | (tmpS >> (32 - SHL));
	tmpM:4 = ~0:4;
	if (ME:1 < MBL:1) goto <invert>;
	tmpM = tmpM << MBL;
	tmpM = tmpM >> ((31-ME) + MBL);
	tmpM = tmpM << (31-ME);
	goto <finish>;
<invert>
	tmpM = tmpM << ME;
	tmpM = tmpM >> ((31-MBL) + ME);
	tmpM = tmpM << (31-MBL);
	tmpM = ~tmpM;
<finish>
	A = zext(tmpA & tmpM) | (A & zext(~tmpM));	
}

