# binutils: a2.d:  514: 7d 4a 3a 87     mfdcr\.  r10,234
:mfdcr. D, DCRN		is $(NOTVLE) & OP=31 & D & DCRN & XOP_1_10=323 & BIT_0=1
{
	D = DCRN;
}

# binutils: a2.d:  55c: 7d 4a 3b 87     mtdcr\.  234,r10
:mtdcr. DCRN, D		is $(NOTVLE) & OP=31 & D & DCRN & XOP_1_10=451 & BIT_0=1
{
	DCRN = D;
}

