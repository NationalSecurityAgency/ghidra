# WS=>  { 0x7, 11, NULL, NULL, 0 },
define pcodeop eratweOp;
:eratwe S,A is $(NOTVLE) & OP=31 & XOP_1_10=211 & S & A & BITS_11_13 & BITS_14_15 & BIT_0=0 { eratweOp(S,A); }

