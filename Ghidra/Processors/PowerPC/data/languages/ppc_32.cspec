        <rule>
          <datatype name="float"/>
          <consume storage="float"/>
        </rule>
        <rule>
          <datatype name="any"/>
          <join/>
        </rule>
      </output>
      <unaffected>
        <register name="r1"/>  <!-- stack pointer -->
        <register name="r2"/>  <!-- _SDA2_BASE_ -->
      	<register name="r13"/> <!-- _SDA_BASE_  -->
        <register name="r14"/>
