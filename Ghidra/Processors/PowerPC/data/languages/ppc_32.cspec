        <pentry minsize="1" maxsize="4">
          <register name="r10"/>
        </pentry>
        <pentry minsize="1" maxsize="500" align="4">
          <addr offset="8" space="stack"/>
        </pentry>
        <rule>
          <datatype name="float"/>
          <consume storage="float"/>
        </rule>
        <rule>
          <datatype name="float"/>
          <goto_stack/>
        </rule>
        <rule>
          <datatype name="struct"/>
          <convert_to_ptr/>
        </rule>
        <rule>
          <datatype name="union"/>
          <convert_to_ptr/>
        </rule>
        <rule>
          <datatype name="any"/>
