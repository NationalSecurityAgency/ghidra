:CMPXCHG^lockx m8,Reg8  is vexMode=0 & lockx & unlock & byte=0xf; byte=0xb0; m8 & Reg8 ...           
{
    build lockx;
    local dest = m8;
    subflags(AL,dest); 
    local diff = AL-dest; 
    resultflags(diff);
    if (ZF) goto <equal>;
    AL = dest;
    goto <inst_end>;
<equal>
    m8 = Reg8;
<inst_end>
    build unlock;
}

