 	tsp = tsp-4;
	MemDestCast(tsp,a2);
}
REGSV_STAT:	",a1-a3"				is (ext_value_areg=3 | ext_value_areg=7) {
 	tsp = tsp-4;
	MemDestCast(tsp,a3);
 	tsp = tsp-4;
	MemDestCast(tsp,a2);
 	tsp = tsp-4;
	MemDestCast(tsp,a1);
}
REGSV_STAT:	",a0-a3"				is ext_value_areg=0xb { 
 	tsp = tsp-4;
	MemDestCast(tsp,a3);
