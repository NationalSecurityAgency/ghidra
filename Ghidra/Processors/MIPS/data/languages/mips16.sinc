	tmp:4 = zext(EXT_IU8);
	tmpa:4 = ext_delay;
	RX32 = (inst_start + tmp - (tmpa << 1)) & 0xFFFFFFFC; 
@ifdef MIPS64
    RX = sext(RX32);
@endif
}
:addiu RX32, pc, EXT_IS8			is ISA_MODE=1 & RELP=1 & ext_isjal=0 & m16_op=0b00001 & ext_is_ext=1 & RX32 & EXT_IS8 & pc & RX {
	tmp:4 = sext(EXT_IS8);
