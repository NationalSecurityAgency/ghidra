	build REGRS_STAT;
}


REGSV_STAT:							is ext_value_areg {}
REGSV_STAT:	",a3"					is (ext_value_areg=1 | ext_value_areg=5 | ext_value_areg=9 | ext_value_areg=0xd) {
 	tsp = tsp-4;
	MemDestCast(tsp,a3);
}
REGSV_STAT:	",a2-a3"					is (ext_value_areg=2 | ext_value_areg=6 | ext_value_areg=0xa) {
 	tsp = tsp-4;
