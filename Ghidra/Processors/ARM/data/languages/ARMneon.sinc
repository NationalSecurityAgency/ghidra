macro replicate1to8(bytes, dest) {
	local val:8 = zext(bytes);
	val = val | (val << 8);
	val = val | (val << 16);
	dest = val | (val << 32);
}

macro replicate2to8(bytes, dest) {
	local val:8 = zext(bytes);
	val = val | (val << 16);
	dest = val | (val << 32);
}

macro replicate4to8(bytes, dest) {
	local val:8 = zext(bytes);
	dest = val | (val << 32);
}

