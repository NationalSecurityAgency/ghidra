  b3:1 = tmp1 - tmp2;
  tmp1 = tmpRn(3);
  tmp2 = tmpRm(3);
  GE4 = tmp2 <= tmp1;
  b4:1 = tmp1 - tmp2;
  Rd = (zext(b4) << 24) | (zext(b3) << 16) | (zext(b2) << 8) | zext(b1);
