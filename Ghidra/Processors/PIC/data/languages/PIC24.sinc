                                                                          #   address  :      code
                                                                          #          0 :  byte2 | byte1
                                                                          #          1 :   pad  | byte3
                                                                          #          2 :  byte2 | byte1
                                                                          #          3 :   pad  | byte3
                                                                          #   .
                                                                          #   .
                                                                          #   .
                                                                          #   etc


